<Window x:Class="IPProcessingTool.ReviewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Review Selections" Height="400" Width="600">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Label Name="ReviewLabel" FontSize="14" FontWeight="Bold" Margin="10"/>
            <Button Name="SubmitButton" Content="Submit" Click="SubmitButton_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
